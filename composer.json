    "autoload-dev": {
        "psr-4": {
            "Shasoft\\CacheInvalidation\\Tests\\": "tests/classes"
        }
    },
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Shabanov Valera",
            "email": "shasoft_cache-invalidation@shasoft.com"
